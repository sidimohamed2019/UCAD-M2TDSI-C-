        function PayHesapla() {
            d = document;

            //html nesnelerinden alınan değerler...
            Mal = parseInt(d.getElementById('MalVarligi').value);
            var ECs = parseInt(d.getElementById('ErkekCocuk').selectedIndex);
            var KCs = parseInt(d.getElementById('KizCocuk').selectedIndex);
            var EKs = parseInt(d.getElementById('ErkekKardes').selectedIndex);
            var KKs = parseInt(d.getElementById('KizKardes').selectedIndex);
            var BabaVar = parseInt(d.getElementById('Baba').selectedIndex);
            var AnneVar = parseInt(d.getElementById('Anne').selectedIndex);
            var EsVar = parseInt(d.getElementById('Es').selectedIndex);

            //hesaplamalarda kullanılacak değişkenler...
            var ECp = 0;
            var KCp = 0;
            var Ap = 0;
            var Bp = 0;
            var EsHesap = 0;
            var Es1p = 0;
            var Es2p = 0;
            var Es3p = 0;
            var Es4p = 0;
            var EKp = 0;
            var KKp = 0;
            var Xp = 0;
            var Mdan = 0;
            var KisiBasiPay = 0;
            var AraHesap = 0;

            //koşullara göre hesaplama işlemleri...
            //alert('if dışı ');
            if (ECs > 0) {
                //1.Koşul:

                //Erkek çocuğun payı:
                ECp = ECs * (2 * (Mal / (2 * ECs + KCs)));
                //Kız çocuğun payı:
                KCp = (KCs > 0 ? KCs * (Mal / (2 * ECs + KCs)) : 0);
            }
            else if (ECs == 0 && KCs >= 2) {
                //2.Koşul:

                KCp = 2 * (Mal / 3);
                //ikinci evre...
                Mdan = Mal - KCp;
                Ap = (AnneVar == 1 ? (1 / 6) * Mdan : 0);
                Bp = (BabaVar == 1 ? (1 / 6) * Mdan : 0);
                //üçüncü evre...
                //Mdan = Mal - (KCp + Ap + Bp); ***(04/09/2012 tarihinde hesap dışı bırakıldı.)
                EsHesap = (EsVar > 0 ? (1 / 8) * Mdan : 0);
            }
            else if (ECs == 0 && KCs == 1) {
                //3.Koşul:

                KCp = Mal / 2;
                //ikinci evre...
                Mdan = Mal - KCp;
                Ap = (AnneVar == 1 ? (1 / 6) * Mdan : 0);
                Bp = (BabaVar == 1 ? (1 / 6) * Mdan : 0);
                //üçüncü evre...
                //Mdan = Mal - (KCp + Ap + Bp); ***(04/09/2012 tarihinde hesap dışı bırakıldı.)
                EsHesap = (EsVar > 0 ? (1 / 8) * Mdan : 0);
            }
            else if (ECs == 0 && KCs == 0 && BabaVar == 1 && EKs == 0 && KKs == 0) {
                //4.Koşul:
                //(03/08/2012 tarihinde değiştirildi. EKs==0 & KKs==0 ilave edildi.)
                Ap = (AnneVar == 1 ? (1 / 3) * Mal : 0);
                //ikinci evre...
                //Mdan = Mal - Ap; ***(04/09/2012 tarihinde itibaren Mdan aşağıdaki gibi hesaplanıyor.)
                Mdan = Mal / 2;
                EsHesap = (EsVar > 0 ? (1 / 4) * Mdan : 0);
                Bp = Mal - (Ap + (EsHesap * EsVar));
            }
            else if (ECs == 0 && KCs == 0 && BabaVar == 0 && AnneVar == 1 && (EKs + KKs) >= 2) {
                //5.Koşul:
                //Eski koşul: (ECs == 0 && KCs == 0 && BabaVar == 0 && AnneVar == 1 && EKs >= 1 && KKs >= 1)

                Ap = (1 / 6) * Mal;
                AraHesap = (1 / 3) * Mal;
                EKp = EKs * (2 * (AraHesap / (2 * EKs + KKs)));
                KKp = KKs * (AraHesap / (2 * EKs + KKs));
                //ikinci evre...
                //Mdan = Mal - (Ap + EKp + KKp); ***(04/09/2012 tarihinde itibaren Mdan aşağıdaki gibi hesaplanıyor.)
                Mdan = Mal / 2;
                EsHesap = (EsVar > 0 ? (1 / 4) * Mdan : 0);
                Ap += Mal - (Ap + EKp + KKp + (EsHesap * EsVar)); //***(04/09/2012 tarihinde ilave edildi.)
            }
            else if (ECs == 0 && KCs == 0 && BabaVar == 0 && AnneVar == 1 && EKs == 1 && KKs == 0) {
                //6.Koşul:

                Ap = (1 / 3) * Mal;
                EKp = (1 / 6) * Mal;
